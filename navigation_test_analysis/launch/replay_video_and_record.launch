<?xml version="1.0"?>
<launch>
    <arg name="videoConfig" />

    <node name="navigation_test_camera_top" pkg="image_view" type="image_view" output="screen">
        <remap from="image" to="/top_camera/image_raw" />
    </node>

<node name="screen_recorder" pkg="navigation_test_analysis" type="screenRecorder.py" output="screen">
    <rosparam command="load" file="$( arg videoConfig )" />
    </node>
</launch>
