<?xml version="1.0"?>
<launch>
    <arg name="videoConfig" />

    <include file="$( find navigation_test_analysis )/launch/replay_video_and_record.launch">
        <arg name="videoConfig" value="$( arg videoConfig )" />
    </include>

    <node name="analyse_bag_file" pkg="navigation_test_analysis" type="worker.py" output="screen" required="true">
        <param name="~filepath" value="$(arg filepath)" />
    </node>
</launch>
